<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <TextView
        android:id="@+id/text_profile1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내정보"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="53dp" />

    <TextView
        android:id="@+id/text_profile_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="아이디"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="124dp" />

    <TextView
        android:id="@+id/text_profile_pw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비밀번호"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="168dp" />

    <TextView
        android:id="@+id/text_profile_idtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test@gmail.com"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="124dp" />

    <TextView
        android:id="@+id/text_profile_pwtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="***********"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="168dp" />

    <Button
        android:id="@+id/button_profile_pw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="PW 수정"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="153dp" />

    <Button
        android:id="@+id/button_profile_dog1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="애완견 1의 정보"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="226dp" />

    <Button
        android:id="@+id/button_profile_dog2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="애완견 2의 정보"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="285dp" />

    <Button
        android:id="@+id/button_profile_dog_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="애완견 추가"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="344dp" />

    <Button
        android:id="@+id/button_profile_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="확인"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="639dp" />
</androidx.constraintlayout.widget.ConstraintLayout>